{
  "name": "brackets-eslint",
  "version": "2.9.10",
  "title": "ESLint",
  "description": "Adds ESLint support to Brackets",
  "keywords": [
    "brackets-extension",
    "eslint",
    "lint",
    "linting",
    "linter"
  ],
  "homepage": "https://github.com/zaggino/brackets-eslint",
  "bugs": "https://github.com/zaggino/brackets-eslint/issues",
  "license": "MIT",
  "author": {
    "name": "Martin Zagora",
    "email": "zaggino@gmail.com",
    "url": "https://github.com/zaggino"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zaggino/brackets-eslint.git"
  },
  "scripts": {
    "build": "rimraf dist && tsc",
    "dev": "tsc --watch",
    "test": "tsc && tslint -c tslint.json --project tsconfig.json",
    "prepublish": "npm run build"
  },
  "dependencies": {
    "brackets-inspection-gutters": "^0.2.7",
    "enable-global-packages": "latest",
    "eslint": "^3.5.0"
  },
  "devDependencies": {
    "@types/jquery": "2.0.32",
    "@types/node": "6.0.39",
    "rimraf": "2.5.4",
    "tslint": "3.15.1",
    "typescript": "2.0.2"
  },
  "engines": {
    "brackets": ">=1.2.0"
  }
}
